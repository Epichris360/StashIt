                                                <h3><a href="/location/show/{{slug}}">{{name}}</a></h3>
                                                <span>{{address}}, {{city}}</span>
                                                <!--
                                                <div class="star-rating" data-rating="3.5">
                                                    <div class="rating-counter">(12 reviews)</div>
                                                </div> -->
                                            </div>
                                        </div>
                                    </div>
                                    
                                    <div class="buttons-to-right">
                                        <a href="/dashboard/active-stash-{{slug}}" class="button gray" >Show Active Stash</a>
                                        <a href="/dashboard/edit-location-{{slug}}" class="button gray"><i class="sl sl-icon-note"></i> Edit</a>
                                        <a href="#" class="button gray"><i class="sl sl-icon-close"></i> Disable</a>
                                    </div>
                                </li>
                                {{/locations}}
                            </ul>
                        </div>
                    </div>


